{
	"name": "svelte-autocomplete-input",
	"version": "0.0.7",
	"description": "Svelte Autocomplete input",
	"license": "MIT",
	"keywords": [
		"svelte",
		"svelteKit",
		"autocomplete",
		"svelte-components",
		"auto complete",
		"autosuggest",
		"auto suggest"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/maxjerry0107/svelte-autocomplete-input"
	},
	"homepage": "https://svelte.dev/repl/1154fae2095b443484589c3194efec16?version=4.2.9",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"exports": {
		"./AutoCompleteInput.svelte": {
			"types": "./dist/AutoCompleteInput.svelte.d.ts",
			"svelte": "./dist/AutoCompleteInput.svelte"
		},
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"publint": "^0.1.9",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-highlight": "^7.4.8",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.11"
	},
	"main": "./dist/index.js",
	"default": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}